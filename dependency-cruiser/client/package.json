{
  "name": "sandbox-dependency-cruiser",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "deps": "depcruise --config .dependency-cruiser.js src",
    "output:svg": "depcruise --config .dependency-cruiser.js src -T dot src | dot -T svg > dependency-graph.svg",
    "codegen": "openapi-typescript ../schema/schema.yaml --export-type --output src/api/api.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.3.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@faker-js/faker": "6.1.2",
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-interactions": "^6.4.20",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/builder-vite": "0.1.24",
    "@storybook/react": "^6.4.20",
    "@storybook/testing-library": "0.0.9",
    "@svgr/rollup": "6.2.1",
    "@types/node": "17.0.23",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/styled-components": "5.1.24",
    "@vitejs/plugin-react": "^1.0.7",
    "babel-loader": "^8.2.4",
    "babel-plugin-styled-components": "2.0.6",
    "csstype": "3.0.11",
    "dependency-cruiser": "11.4.1",
    "esbuild-register": "3.3.2",
    "openapi-typescript": "5.2.0",
    "the-new-css-reset": "1.5.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.0"
  }
}
